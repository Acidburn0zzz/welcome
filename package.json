{
  "name": "welcome",
  "version": "0.35.2",
  "description": "Welcome users to Atom with useful information",
  "main": "./lib/main",
  "repository": "https://github.com/atom/welcome",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "scripts": {
    "test": "standard lib/**/*.js test/**/*.js && atom --test test/*.test.js"
  },
  "atomTestRunner": "atom-mocha-test-runner",
  "atomTranspilers": [
    {
      "glob": "{lib,test}/**/*.js",
      "transpiler": "atom-babel6-transpiler",
      "options": {
        "cacheKeyFiles": [
          "package.json",
          ".babelrc"
        ]
      }
    }
  ],
  "consumedServices": {
    "metrics-reporter": {
      "versions": {
        "^1.0.0": "consumeReporter"
      }
    }
  },
  "configSchema": {
    "showOnStartup": {
      "type": "boolean",
      "default": true,
      "description": "Show welcome panes with useful information when opening a new Atom window."
    }
  },
  "deserializers": {
    "WelcomeView": "createWelcomeView",
    "GuideView": "createGuideView",
    "ConsentView": "createConsentView"
  },
  "dependencies": {
    "etch": "^0.8.0",
    "atom-babel6-transpiler": "0.0.3",
    "babel-core": "^6.21.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-react": "^6.16.0"
  },
  "devDependencies": {
    "atom-mocha-test-runner": "^1.0.0",
    "standard": "^8.6.0"
  },
  "standard": {
    "globals": ["atom"]
  }
}
